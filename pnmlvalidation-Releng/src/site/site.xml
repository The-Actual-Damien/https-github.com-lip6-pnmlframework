<project xmlns="http://maven.apache.org/DECORATION/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd"
	name="PNML Document Checker">

	<publishDate position="right" format="yyyy-MM-dd" />
	<version position="right" />

	<skin>
		<groupId>lt.velykis.maven.skins</groupId>
		<artifactId>reflow-maven-skin</artifactId>
		<version>1.1.1</version>
	</skin>

	<custom>
		<reflowSkin>
			<smoothScroll>true</smoothScroll>
			<theme>bootswatch-readable</theme>
			<highlightJs>true</highlightJs>
			<highlightJsTheme>github</highlightJsTheme>
			<brand>
				<name>PNML Check</name>
				<href>http://pnml.lip6.fr/pnmlvalidation/</href>
			</brand>
			<slogan>Free Tool to Check your PNML Documents against the ISO/IEC
				15909-2 Specifications</slogan>
			<titleTemplate>%2$s | %1$s</titleTemplate>
			<toc>top</toc>
			<tocTopMax>6</tocTopMax>
			<topNav>Download|Documentation|reports</topNav>
			<bottomNav maxSpan="9">
				<column>Overview|Find out more</column>
				<column>Download</column>
				<column>Documentation</column>
				<column>reports|modules</column>
			</bottomNav>
			<bottomDescription>
			<![CDATA[
				 <span class="color-highlight">PNML Check</span> is a simple command-line tool to check your <a href="http://www.pnml.org">Petri Net Markup Language</a> Documents againts 
				the <a href="http://www.iso.org/iso/catalogue_detail.htm?csnumber=43538">ISO/IEC 15909-2</a> Specifications (metamodels and constraints). 
			 ]]>
			</bottomDescription>
			<pages>
				<index project="pnmlvalidation-Releng">
					<titleTemplate>PNML Document Checker</titleTemplate>
					<shortTitle>Welcome</shortTitle>
					<breadcrumbs>false</breadcrumbs>
					<toc>false</toc>
					<markPageHeader>false</markPageHeader>
					<sections>
						<carousel />
						<body />
						<sidebar />
						<thumbs>2</thumbs>
						<columns>2</columns>
					</sections>
				</index>
				<source-repository>
					<highlightJs>false</highlightJs>
				</source-repository>
				<issue-tracking>
					<highlightJs>false</highlightJs>
					<toc>false</toc>
				</issue-tracking>
				<license>
					<highlightJs>false</highlightJs>
					<toc>false</toc>
				</license>
				<project-info>
					<toc>false</toc>
				</project-info>
				<github-report>
					<toc>false</toc>
				</github-report>
				<dependencies>
					<tocTopMax>4</tocTopMax>
				</dependencies>
				<developer-info>
					<toc>sidebar</toc>
				</developer-info>
			</pages>
		</reflowSkin>
	</custom>

	<body>
		<head>
			<link rel="author" href="http://lip6.fr/Lom-Messan.Hillah" />
		</head>

		<links>
			<item name="Download" href="download.html" />
			<item name="Project on GitHub" href="https://github.com/lhillah/pnmlframework/tree/master/pnmlvalidation-Releng" />
		</links>

		<breadcrumbs>
			<item name="PNML Check" href="/" />
		</breadcrumbs>

		<menu name="Overview" inherit="top">
			<item name="Home" href="./" />
			<item name="Changelog" href="changelog.html" />
		</menu>
		<menu name="Download" inherit="top">
			<item name="Download" href="download.html" />
		</menu>
		<menu name="Documentation" inherit="bottom">
			<item name="Usage" href="usage.html" />
		</menu>
		<menu name="Find out more" inherit="bottom">
			<item name="Project on GitHub" href="https://github.com/lhillah/pnmlframework/tree/master/pnmlvalidation-Releng" />
			<item name="PNML Framework" href="http://pnml.lip6.fr" />
			<item name="PNML.ORG" href="http://www.pnml.org" />
		</menu>
		<menu name="Contribute" inherit="bottom">
			<item name="Contribute" href="contribute.html" />
			<item name="Issues"
				href="https://github.com/lhillah/pnmlframework/issues" />
			<item name="Fork on GitHub" href="https://github.com/lhillah/pnmlframework" />
		</menu>
		<menu name="Maven documentation" ref="reports" inherit="bottom" />
	</body>


</project>
